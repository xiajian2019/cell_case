<!DOCTYPE html>
<html>
<head>
  <title>CellCase</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="navbar navbar-default navbar-fixed-top">
  <div class="container">
    <div class="navbar-brand">
      <%= link_to "UI组件试验田", '/'%>
    </div>

    <div class='nav-collapse'>
      <ul class='nav navbar-nav navbar-right'>
        <li><%= link_to 'Cell Todo Example', dash_index_path %></li>
        <li class='dropdown'>
          <a href="#" class='dropdown-toggle' data-toggle='dropdown'>
            活动页面系统
            <b class='caret'></b>
          </a>
          <ul class='dropdown-menu'>
            <li><%= link_to "页面", pages_path %></li>
            <li><%= link_to "组件", components_path %></li>
          </ul>
        </li>
        </li>
        <li><%= link_to "Actionview Component", demo_index_path %></li>
      </ul>
    </div>
  </div>
</div>

<div class="container" style="margin-top: 70px">
  <%= yield %>
</div>

<%= javascript_include_tag 'application' %>
<%= include_gon %>
<script src="https://cdn.bootcss.com/clipboard.js/2.0.1/clipboard.min.js"></script>
<script src="https://cdn.bootcss.com/ace/1.4.5/ace.js"></script>
<script src="https://cdn.bootcss.com/ace/1.4.5/ext-language_tools.js"></script>
<script src="https://cdn.bootcss.com/json-editor/0.7.28/jsoneditor.min.js"></script>
<%= yield :page_javascript %>
</body>
</html>
